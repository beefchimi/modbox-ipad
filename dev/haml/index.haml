	= Haml::Engine.new(File.read("dev/haml/partials/_doc_head.haml")).render

	%body

		#svgInject
			/ inject:svg
			/ endinject

		%main{:role => "main"}

			%section{"data-container" => "width_480"}

				%article

					.wrap_svg
						%svg.stc_logo{:viewBox => "0 0 318 420"}
							%use{"xlink:href" => "#stc_logo"}

					%h1.visuallyhidden march&eacute; st-charles market

					%form#mc-embedded-subscribe-form.validate{:action => "//modbox.us10.list-manage.com/subscribe/post-json?u=9d04fb276b7023a982943ca3e&amp;id=01bf434b00&c=?", :method => "get", :name => "mc-embedded-subscribe-form", :novalidate => true}

						%h2 Please enter your email address to receive information about St. Charles Market.

						.wrap_inputs{"data-container" => "width_340"}
							%input#mce-NAME.mce-name{:type => "text", :name => "NAME", :value => "", :placeholder => "Name", :tabindex => "1"}
							%input#mce-EMAIL.mce-email{:type => "email", :name => "EMAIL", :value => "", :placeholder => "Email", :required => true, :tabindex => "2"}

						.wrap_boxes

							%p I'm interested in:

							.wrap_checkbox-1
								%input.visuallyhidden{:id => "mce-group[5877]-5877-0", :type => "checkbox", :name => "group[5877][1]", :value => "1", :tabindex => "3"}
								%label{:for => "mce-group[5877]-5877-0"}
									%span Events
									%svg.ui_checkmark{:viewBox => "0 0 64 64"}
										%use{"xlink:href" => "#ui_checkmark"}

							.wrap_checkbox-2
								%input.visuallyhidden{:id => "mce-group[5877]-5877-1", :type => "checkbox", :name => "group[5877][2]", :value => "2", :tabindex => "4"}
								%label{:for => "mce-group[5877]-5877-1"}
									%span Development
									%svg.ui_checkmark{:viewBox => "0 0 64 64"}
										%use{"xlink:href" => "#ui_checkmark"}

						#do-not-remove.visuallyhidden
							%input{:type => "text", :name => "b_9d04fb276b7023a982943ca3e_01bf434b00", :value => "", :tabindex => "-1"}

						-# %button#mc-embedded-subscribe.button{:type => "submit", :value => "Subscribe", :name => "subscribe", :tabindex => "5"}
						%input#mc-embedded-subscribe{:type => "submit", :value => "Subscribe", :name => "subscribe"}


		%aside#overlay{"data-overlay" => "inactive"}
			#overlay_modal.overlay_modal
				.wrap_response
					%h4#mce-response-text
					%a#close{:href => "#", :title => "Close Window"} Close


